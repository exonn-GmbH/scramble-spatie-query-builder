{
    "name": "exonn-gmbh/scramble-spatie-query-builder",
    "description": "This is the Scramble extension, which detects the usage of the Spatie query builder and adds applicable query parameters to the openapi definitions",
    "keywords": [
        "extension",
        "spatie",
        "query-builder",
        "scramble",
        "openapi"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Exonn\\ScrambleSpatieQueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Exonn\\ScrambleSpatieQueryBuilder\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Volodymyr Rudomanenko",
            "email": "rudomanenkovladimir@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "dedoc/scramble": "^0.9.0",
        "spatie/laravel-query-builder": "^5.0"
    },
    "require-dev": {
        "laravel/pint": "^v1.1.0",
        "pestphp/pest": "^2.34",
        "nunomaduro/collision": "^7.0|^8.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
